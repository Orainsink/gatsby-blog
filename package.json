  "scripts": {
    "build:performance": "npx process-top ./node_modules/.bin/gatsby build",
    "build": "gatsby build",
    "format": "prettier --write \"**/*.{js,jsx,json,md,mdx,ts,tsx}\"",
    "start": "gatsby develop",
    "serve": "gatsby serve",
    "clean": "gatsby clean",
    "check-types": "tsc --noEmit --project ./tsconfig.json",
    "new": "ts-node ./scripts/newPost.ts",
    "test": "echo \"Write tests! -> https://gatsby.dev/unit-testing\" ",
